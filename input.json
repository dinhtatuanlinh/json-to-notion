        },
        "distribution_start_date": {
          "type": "string",
          "required": true,
          "description": "Start date of reward distribution"
        },
        "distribution_start_time": {
          "type": "string",
          "required": true,
          "description": "Start time of reward distribution"
        },
        "distribution_end_date": {
          "type": "string",
          "required": true,
